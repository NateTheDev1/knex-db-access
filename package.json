{
  "name": "knex-db-access",
  "version": "2.0.0",
  "description": "An easy way to access your SQL database without queries",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" ",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "postversion": "git push && git push --tags"
  },
  "author": "Nathaniel Richards <nathaniel-richards@lambdastudents.com> (https://nathanielrichards.dev)",
  "license": "ISC",
  "homepage": "https://github.com/NateTheDev1/knex-db-access",
  "keywords": [
    "automation",
    "database access",
    "knex",
    "knex schema",
    "sql queries",
    "postgres queries"
  ],
  "devDependencies": {
    "@types/node": "^14.11.4",
    "prettier": "^2.1.2",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "knex": "^0.21.6"
  }
}
